  <!-- <link rel="stylesheet" href="/main.css"> -->
  <link rel="stylesheet" href="/app.css">
  <script src="/app.js" defer></script>

  <title>Title </title>
</head>
<body class="bg-gray">
  <%- include('partials/header') %> 
  <div class="container bg-white my-5 px-4 py-4">
    <h1><%= listing.listingName %> </h1>
